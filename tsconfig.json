{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "es6",
    "rootDir": "src/",
    "outDir": "dist/",
    "jsx": "react",
    "lib": ["es6", "dom", "es2017.object"],
    "types": [
      "react",
      "vss-web-extension-sdk"
    ]
  },
  "exclude": ["node_modules", "typings"]
}
